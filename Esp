                            local oh1 = v.Character.Head
                            local oh2 = v.Character.Head.Position * Vector3.new(L_64_, L_65_, L_66_) + Vector3.new(L_61_, L_62_, L_63_)
                            local oh3 = 'CZ'
                            local oh5 = nil
